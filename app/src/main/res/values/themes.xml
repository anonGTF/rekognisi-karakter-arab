<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Swipe" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
    </style>
    <style name="HeaderTitle.Hide" parent="@style/Widget.MaterialComponents.MaterialCalendar.HeaderTitle">
        <item name="android:visibility">gone</item>
    </style>

    <style name="ShapeAppearance.App.MediumComponent" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">@dimen/dimen_radius_card</item>
    </style>

    <style name="MaterialDateHeaderSelection" parent="@style/Widget.MaterialComponents.MaterialCalendar.HeaderSelection">
        <item name="android:fontFamily">@font/opensans_semibold</item>
        <item name="android:textSize">@dimen/_20ssp</item>
    </style>

    <style name="Widget.App.MaterialCalendar.DayTextView" parent="Widget.MaterialComponents.MaterialCalendar.DayTextView">
        <item name="android:textAppearance">@style/Caption.Regular.12</item>
    </style>

    <style name="MyDatePickerStyle" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/black_text_60</item>
        <!--selected Item color-->
        <item name="colorControlActivated">#2196F3</item>
        <!-- arrow bordercolor(<>)-->
        <item name="android:selectableItemBackgroundBorderless">@color/purple_secondary</item>
        <!-- Highlight item color-->
        <item name="colorControlHighlight">#d50000</item>
        <!--Calender Background color -->
        <item name="android:windowBackground">@color/orange_primary</item>
        <!-- Ok Cancel Color-->
        <item name="android:textColor">#4CAF50</item>
        <!-- Week TextColor-->
        <item name="android:textColorSecondary">#E91E63</item>
        <!-- Calender Number color arrow color (< >) -->
        <item name="android:textColorPrimary">#3F51B5</item>
        <!--day , month-->
        <item name="android:textColorPrimaryInverse">#0ff</item>
        <item name="android:textSize">@dimen/_14ssp</item>
    </style>

    <!--below is the style for transparent activity and here we are using no action bar.-->
    <style name="Theme.AppCompat.Translucent" parent="Theme.AppCompat.NoActionBar">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>

    <style name="customActionBar" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:titleTextStyle">@style/customActionBar.Text</item>
    </style>

    <style name="customActionBar.Text" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Theme.RemoveSplashScreenTheme" parent="@style/Theme.Swipe">
